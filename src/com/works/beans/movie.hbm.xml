<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping.dtd">
<hibernate-mapping>
	<class name="com.works.beans.Movies" table="movies">
		<id name="id" column="MovieID" type="int">
			<generator class="increment" />
		</id>
		<property name="name" column="MovieName" type="string" />
		<property name="price" column="MoviePrice" type="double" />
	</class>
</hibernate-mapping>
